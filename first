import time
print('Добро пожаловать в словарь современных слов, с помощью данной программы вы можете узнать незнакомые вам современные слова')
time.sleep(3)
while True:
    meme_dict = {
                "КРИНЖ": "Что-то очень странное или стыдное",
                "ЛОЛ": "Что-то очень смешное",
                "ФРИК": 'Кто-то неадекватный/странный',
                "РОФЛ": 'Шутка(рофлить - шутить)',
                "КРИПОВЫЙ": 'Cтрашный, пугающий',
                "Агриться": 'Злиться'
                }
    word = input("Введите непонятное слово (большими буквами!): ")
    if word in meme_dict.keys():
        print(meme_dict[word])
    else:
        print('такого слова не найденно')
